{
  "name": "opsway/doctrine-dbal-postgresql",
  "description": "Extensions for support Postgres in Doctrine DBAL & DQL",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Vronskiy",
      "email": "alvro@opsway.com"
    }
  ],
  "require": {
    "php": "^5.6 | ^7.0",
    "doctrine/dbal": "~2.4",
    "doctrine/orm": "~2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 | ^6.5"
  },
  "autoload": {
    "psr-4": {
      "Opsway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpsWay\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test-cover": "phpunit --coverage-clover=coverage.xml"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
