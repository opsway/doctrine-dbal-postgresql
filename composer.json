{
  "name": "opsway/doctrine-dbal-postgresql",
  "description": "Extensions for support Postgres in Doctrine DBAL & DQL",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Vronskiy",
      "email": "alvro@opsway.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "doctrine/dbal": "~2.4",
    "doctrine/orm": "~2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "OpsWay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpsWayTest\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0-dev"
    }
  }
}
